<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fund.infras.dao.mapper.UserMapper">

    <select id="selectUserByPage" resultType="com.fund.infras.dao.model.FundUserPO">
        select
            `id`,`state`,`name`,`head_img_url`,`phone_number`,`utc_create`
        from
            `fund_user`
        where
            `utc_deleted` = 0
    </select>

</mapper>